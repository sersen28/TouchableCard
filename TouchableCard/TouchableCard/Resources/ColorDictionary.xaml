	<SolidColorBrush x:Key="MenuMouseOver" Color="#333333" />
	<SolidColorBrush x:Key="MenuMousePressed" Color="#141414" />

	<SolidColorBrush x:Key="ButtonBright" Color="#9aeba3" />
